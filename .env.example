# CogniSense Environment Variables
# Copy this file to .env and fill in your values

# ── App ──────────────────────────────────────────────
APP_NAME=CogniSense
ENVIRONMENT=development
DEBUG=true

# ── Database ─────────────────────────────────────────
DATABASE_URL=sqlite:///./cognisense.db

# ── Logging ──────────────────────────────────────────
LOG_LEVEL=INFO

# ── Capture ──────────────────────────────────────────
WEBCAM_INDEX=0
AUDIO_ENABLED=false
CAPTURE_FPS=15

# ── Frontend ─────────────────────────────────────────
REACT_APP_API_URL=http://localhost:8000/api/v1
